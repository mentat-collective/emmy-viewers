{:paths ["src"]
 :deps
 {io.github.nextjournal/clerk
  {:git/sha "75f073c4899e37b2259cb2057cbf863b90892897"}
  ;; sicmutils/sicmutils {:mvn/version "0.22.0"}
  sicmutils/sicmutils {:local/root "../sicmutils"}
  org.mentat/jsxgraph.cljs {:mvn/version "0.1.0"}
  org.mentat/mathbox.cljs {:mvn/version "0.0.1"}
  org.mentat/mathlive.cljs {:mvn/version "0.1.0"}
  org.mentat/clerk-utils {:mvn/version "0.1.0"}}

 :aliases
 {:dev
  {:extra-paths ["dev"]
   :extra-deps
   {org.clojure/clojurescript {:mvn/version "1.11.60"}
    ;; This needs to match the version declared in `package.json`.
    thheller/shadow-cljs {:mvn/version "2.20.14"}}}

  :nextjournal/clerk
  {:extra-deps
   {io.github.nextjournal/clerk.render
    {:git/url "https://github.com/nextjournal/clerk"
     :git/sha "75f073c4899e37b2259cb2057cbf863b90892897"
     :deps/root "render"}
    org.mentat/clerk-utils {:mvn/version "0.1.0"}}
   :exec-fn user/garden!}

  :build
  {:deps {io.github.clojure/tools.build {:git/tag "v0.8.2" :git/sha "ba1a2bf"}
          slipset/deps-deploy {:mvn/version "0.2.0"}}
   :ns-default build}

  :lint
  ;; Invoke with clj -M:dev:lint --lint src
  {:main-opts ["-m" "clj-kondo.main"]
   :replace-deps {clj-kondo/clj-kondo {:mvn/version "RELEASE"}}}

  :local
  {:replace-deps
   {io.github.nextjournal/clerk {:local/root "../clerk"}
    sicmutils/sicmutils {:local/root "../sicmutils"}}}}}
