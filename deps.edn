{:paths ["src" "resources"]
 :deps
 {org.clojure/data.json {:mvn/version "2.4.0"}
  org.mentat/emmy {:mvn/version "0.30.0"}
  org.mentat/jsxgraph.cljs {:mvn/version "0.2.0"}
  org.mentat/leva.cljs {:local/root "../leva.cljs" #_#_:mvn/version "0.2.2"}
