  :portal
  {:extra-paths ["dev"]
   :extra-deps
   {djblue/portal {:mvn/version "0.42.0"}}}

  :build
  {:deps
   {io.github.clojure/tools.build {:git/tag "v0.9.4" :git/sha "76b78fe"}
